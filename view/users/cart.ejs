                                    <script>
    function increaseQuantity(productId, total, price) {
    var currSubtotal = document.getElementById(`subTotal_${productId}`);
    var currentQuantityElement = document.getElementById(`qty_${productId}`);
    var currentQuantityValue = parseInt(currentQuantityElement.innerText);

    currentQuantityValue++;
    currentQuantityElement.innerText = currentQuantityValue;
    currSubtotal.innerText = `₹${currentQuantityValue * price}`;

    // Recalculate and update the total amount
    
    // Add logic to update the cart on the server when the quantity changes
    updateCartItemQuantity(productId, 'up');
}

function decreaseQuantity(productId, total, price) {
    var currSubtotal = document.getElementById(`subTotal_${productId}`);
    var currentQuantityElement = document.getElementById(`qty_${productId}`);
    var currentQuantityValue = parseInt(currentQuantityElement.innerText);

    if (currentQuantityValue > 1) {
        currentQuantityValue--;
        currentQuantityElement.innerText = currentQuantityValue;
        currSubtotal.innerText = `₹${currentQuantityValue * price}`;

        // Recalculate and update the total amount
       
        // Add logic to update the cart on the server when the quantity changes
        updateCartItemQuantity(productId, 'down');
    }
}



