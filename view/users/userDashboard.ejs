                                // JavaScript to fetch and display wallet balance
                                document.getElementById('viewBalanceLink').addEventListener('click', function(event) {
                                  event.preventDefault(); // Prevent the default action of the link
                                  fetchWalletBalance(); // Call function to fetch wallet balance
                                });
                              
                                function fetchWalletBalance() {

                                    fetch('/Wallet')
                                 .then(response => {
                                   if (!response.ok) {
                                 throw new Error('Failed to fetch wallet balance');
                                   }
                                 return response.json();
                                 })
                                .then(data => {
                                   // Update modal with fetched wallet balance
                                  document.getElementById('walletBalance').innerHTML = 'Available balance : Rs  ' + data.balance;
                                })
                                 .catch(error => {
                                   console.error(error);
                                  // Display error message in modal
                                 document.getElementById('walletBalance').innerHTML = 'Available balance : Rs 0 . 00';
                                });
                                }
                              </script>


                                      
     
                                                            
                                                            


