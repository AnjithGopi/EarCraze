                                                                            <% } else if(item.orderStatus=='Pending'){ %>
                                                                                <span class="badge bg-warning text-dark"><%=item.orderStatus%></span>
                                                                            <% } else if(item.orderStatus=='Shipped'){ %>
                                                                                <span class="badge bg-primary"><%=item.orderStatus%></span>
                                                                            <% } else if(item.orderStatus=='Delivered'){ %>
                                                                                <span class="badge bg-success"><%=item.orderStatus%></span>
                                                                            <% } else if(item.orderStatus=='Returned'){ %>
                                                                                <span class="badge bg-secondary"><%=item.orderStatus%></span>
                                                                            <% } else { %>
                                                                                <span class="badge bg-info"><%=item.orderStatus%></span>
                                                                            <% } %>
                                                                        </td>
                                                                        <% if(item.paymentStatus=='Recieved'){ %>
                                                                            <td><span class="badge bg-success"><%=item.paymentStatus%></span></td> 
                                                                        <% } else { %>
                                                                            <td><span class="badge bg-warning text-dark"><%=item.paymentStatus%></span></td> 
                                                                        <% } %>
                                                                        <td style="font-size: 11px;"><%= item.orderDate.toLocaleDateString() %></td>
                                                                        <td style="font-size:11px;">₹ <%= parseFloat(item.totalAmount).toLocaleString() %></td>
                                                                        <td>
                                                                            <div class="btn-group" role="group">
                                                                                <% if(item.orderStatus=='Delivered'){ %>
                                                                                    <a href="/returnOrder?id=<%= item._id %>" class="btn btn-sm btn-outline-dark btn-cancel">Return</a>
                                                                                <% } else if(item.orderStatus=='Returned'){ %>
                                                                                    <span class="badge bg-secondary">Returned</span>
                                                                                <% } else if (item.orderStatus=="Cancelled"){%>
                                                                                    <span class="badge bg-secondary">Cancelled</span>

                                                                                <% }else {%>
                                                                                    <a href="/cancelOrder?id=<%= item._id %>" class="btn btn-sm btn-outline-danger btn-cancel">Cancel</a>

                                                                               <% } %>
                                                                                   
                                                                                
                                                                            </div>
                                                                        </td>
                                                                        <td>
                                                                            <a href="/userOrderDetails?id=<%=item._id%> " class="btn btn-sm btn-outline-dark btn-order-details">Details</a>
                                                                        </td>
                                                                    </tr>
                                                                <% }) %>
                                                            <% } else { %>
                                                                <tr>
                                                                    <td colspan="8">No orders found.</td>
                                                                </tr>
                                                            <% } %>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    
                                   
                        
                                    
                                    
                                    
                                       
                                    <div class="tab-pane fade" id="address" role="tabpanel" aria-labelledby="address-tab">
                                        <div class="row">
                                            <%if(locals.showAddress){%>
                                            <div class="col-lg-6">
                                                <%showAddress.forEach((address)=>{%>
                                                <div class="card">
                                                    
                                                    <div class="card-header">
                                                        <h5 class="mb-0">Shipping Address</h5>
                                                    </div>
                                                    <div class="card-body">
                                                        <p>Name :<%=address.first_name%> <%=address.last_name%></p><br>
                                                        <address>Address :<%=address.address%><br>
                                                            City :<%=address.city%><br>State :<%=address.state%><br>Phone : <%=address.mobile%></address>
                                                        <p>Zip :<%=address.zipcode%></p>
                                                        <a href="#" class="btn-small">Edit</a>
                                                    </div>
                                                </div>
                                                <br>
                                                <br>
                                                <%})%>
                                                <%}%>
                                            </div>
                                       
                                            <div class="col-md-6 text-start" style="padding-bottom: 20px;">
                                                <a href="/addressForm" class="btn btn-success add-address-button">
                                                  <i class="bi bi-plus"></i> Add New Address
                                                </a>
                                            </div>
                                        </div>
                                        <br>
                                    </div>
                                    
                                   
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
